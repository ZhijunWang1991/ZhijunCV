##Caucalate significant of pulp
pulp_C <- pulp$X.13C
pulp_Farm <- pulp$Farm
pulp_mean <- aggregate(pulp_C, by =list(pulp_Farm), FUN=mean)
pulp_sd <- aggregate(pulp_C, by =list(pulp_Farm), FUN=sd)
