table_pulp <- group_by(pulp, Farm) %>%
summarise(
count = n(),
mean = mean(X.13C, na.rm = TRUE),
sd = sd(X.13C, na.rm = TRUE)
)
##show results
write.csv(table_pulp, file = "pulp_C.csv")
